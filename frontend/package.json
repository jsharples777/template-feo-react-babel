{
  "name": "template-feo-react-babel",
  "version": "1.0.0",
  "description": "Template for front-end with React and Babel compiler",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "test": "jest",
    "compile": "babel src --out-dir ../public/js  --verbose --extensions '.jsx,.js,.ts,.tsx'",
    "watch": "babel src --out-dir ../public/js --watch --verbose  --extensions '.jsx,.js,.ts,.tsx'"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "socket.io": "^4.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@types/jest": "^26.0.24",
    "babel-polyfill": "^6.26.0",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.4",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsharples777/template-feo-react-babel.git"
  },
  "keywords": [
    "React",
    "Babel"
  ],
  "author": "Jim Sharples",
  "license": "ISC",
