{
  "name": "template-feo-react-babel",
  "version": "1.0.0",
  "description": "Template for front-end with React and Babel compiler",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "test": "jest",
    "build": "webpack --mode production",
    "webpack_compile": "webpack --mode development",
    "webpack_watch": "webpack --mode development --watch",
    "compile": "babel src --out-dir ../backend/public/js  --verbose --extensions '.jsx,.js,.ts,.tsx'",
    "watch": "babel src --out-dir ../backend/public/js --watch --verbose  --extensions '.jsx,.js,.ts,.tsx'",
    "lint": "eslint --ext js,jsx,tsx,ts --fix ."
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "debug": "^4.3.2",
    "jquery": "^3.6.0",
    "path": "^0.12.7",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "socket.io": "^4.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@types/jest": "^26.0.24",
    "@types/react": "^17.0.15",
    "@types/react-dom": "^17.0.9",
    "babel-loader": "^8.2.2",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-typescript": "^0.14.0",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsharples777/template-feo-react-babel.git"
  },
  "keywords": [
    "React",
    "Babel"
  ],
  "author": "Jim Sharples",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jsharples777/template-feo-react-babel/issues"
  },
  "homepage": "https://github.com/jsharples777/template-feo-react-babel#readme",
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  }
}
