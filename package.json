{
  "name": "template-feo-react-babel",
  "version": "1.0.0",
  "description": "Template for front-end with React and Babel compiler",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "test": "jest",
    "compilefe": "babel frontend/src --out-dir public/js  --verbose --extensions '.jsx,.js,.ts,.tsx'",
    "watchfe": "babel frontend/src --out-dir public/js --watch --verbose  --extensions '.jsx,.js,.ts,.tsx'",
    "compilebe": "babel backend/src --out-dir backend/dist  --verbose --extensions '.jsx,.js,.ts,.tsx'",
    "watchbe": "babel backend/src --out-dir backend/dist --watch --verbose  --extensions '.jsx,.js,.ts,.tsx'",
    "start": "nodemon -e js,graphql,env -w backend -w .env  -w . backend/dist/server.js"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "bootstrap": "^4.6.0",
    "chai": "^4.3.4",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.5",
    "debug": "^4.3.2",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-handlebars": "^5.3.2",
    "express-session": "^1.17.2",
    "http": "0.0.1-security",
    "http-errors": "~1.6.3",
    "jquery": "^3.6.0",
    "jquery-ui": "^1.12.1",
    "mocha": "^9.0.2",
    "mongodb": "^4.0.0",
    "mongoose": "^5.13.2",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.1.0",
    "path": "^0.12.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "request": "^2.88.2",
    "serve-favicon": "^2.5.0",
    "should": "^13.2.3",
    "socket.io": "^4.1.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@types/jest": "^26.0.24",
    "@types/mongodb": "^4.0.7",
    "@types/node": "^16.4.7",
    "babel-polyfill": "^6.26.0",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.4",
    "tslint": "^6.1.3",
    "typescript": "^4.3.5"
  },
