{
  "name": "template-feo-react-babel",
  "version": "1.0.0",
  "description": "Template for front-end with React and Babel compiler",
  "main": "index.js",
  "scripts": {
    "tsc": "tsc",
    "compile": "babel --extensions \".ts,.js,.jsx,.tsx\" frontend/src --out-dir public/js",
    "watch": "babel frontend/src --out-dir public/js --watch --verbose",
    "start": "nodemon -e js,graphql,env -w server -w .env  -w . server/server.js"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "bootstrap": "^4.6.0",
    "bulma": "^0.9.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "request": "^2.88.2",
    "systemjs": "^6.10.1",
    "tsc": "^2.0.3",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.7",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@types/react": "^17.0.13",
    "@types/react-dom": "^17.0.8",
    "babel-polyfill": "^6.26.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jsharples777/template-feo-react-babel.git"
  },
  "keywords": [
    "React",
    "Babel"
  ],
  "author": "Jim Sharples",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jsharples777/template-feo-react-babel/issues"
  },
  "homepage": "https://github.com/jsharples777/template-feo-react-babel#readme"
}
